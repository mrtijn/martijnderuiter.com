{
  "private": true,
  "scripts": {
    "start": "node ./server/index.js",
    "dev": "npm run server & npm run build-js & npm run build-css",
    "build-js": "watchify -vd -p  browserify-hmr -e app/app.js -o assets/js/build.js -t [babelify --presets [ es2015 stage-2 ]]",
    "build-css": "node-sass -u autoprefixer app/styles/build.scss  assets/css/style.css -w -r",
    "server": "lite-server",
    "build": " browserify app/app.js | uglifyjs -c warnings=false -m > assets/js/build.js",
    "assets": "cp node_modules/normalize.css/normalize.css assets"
  },
  "dependencies": {
    "gsap": "^1.18.5",
    "jquery": "^2.2.4",
    "mattboldt.typed.js": "^1.1.4",
    "vue": "^2.0.2",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.3.0",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-runtime": "^5.8.0",
    "babelify": "^7.3.0",
    "browserify": "^12.0.1",
    "browserify-hmr": "^0.3.1",
    "lite-server": "^2.2.0",
    "node-sass": "^3.7.0",
    "three": "^0.84.0",
    "uglify-js": "^2.5.0",
    "vue-hot-reload-api": "^1.2.2",
    "vueify": "^9.2.4",
    "vueify-insert-css": "^1.0.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
